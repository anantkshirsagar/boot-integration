<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="in.sb.mybatis.dao.IStudentDAO">

	<insert id="addStudent"
		parameterType="in.sb.mybatis.model.Student">
		insert into student(id, name, course) values(#{id},
		#{name}, #{course});
	</insert>
</mapper>